//  121 // POWER CONSERVATION DEFINITIONS
//  122 // Sleep mode H/W definitions (enabled with POWER_SAVING compile option).
//  123 #define CC2540_PM0                          0                     // PM0, Clock oscillators on, voltage regulator on
//  124 #define CC2540_PM1                          1                     // PM1, 32.768 kHz oscillators on, voltage regulator on
//  125 #define CC2540_PM2                          2                     // PM2, 32.768 kHz oscillators on, voltage regulator off
//  126 #define CC2540_PM3                          3                     // PM3, All clock oscillators off, voltage regulator off
//  127 
//  128 // HAL power management mode is set according to the power management state.
//  129 // The default setting is HAL_SLEEP_OFF. The actual value is tailored to
//  130 // different HW platform. Both HAL_SLEEP_TIMER and HAL_SLEEP_DEEP selections
//  131 // will turn off the system clock, and halt the MCU. HAL_SLEEP_TIMER can be
//  132 // woken up by sleep timer interrupt, I/O interrupt and reset. HAL_SLEEP_DEEP
//  133 // can be woken up by I/O interrupt and reset.
//  134 #define HAL_SLEEP_OFF                       CC2540_PM0
//  135 #define HAL_SLEEP_TIMER                     CC2540_PM2
//  136 #define HAL_SLEEP_DEEP                      CC2540_PM3
//  137 
//  138 // MAX_SLEEP_TIME calculation:
//  139 // Sleep timer maximum duration = 0xFFFF7F / 32768 Hz = 511.996 seconds
//  140 // Round it to 510 seconds or 510000 ms
//  141 #define MAX_SLEEP_TIME                      16711680              // max time to sleep allowed by ST, in 32kHz ticks
//  142 
//  143 // Minimum time to sleep:
//  144 // 1. avoid thrashing in-and-out of sleep with short OSAL timer
//  145 // 2. define minimum safe sleep period
//  146 #if !defined (PM_MIN_SLEEP_TIME)
//  147 #define PM_MIN_SLEEP_TIME                   66                    // default to min safe sleep time, in 32kHz ticks
//  148 #endif // !PM_MIN_SLEEP_TIME
//  149 
//  150 // This value is used to adjust the sleep timer compare value such that the
//  151 // sleep timer compare takes into account the amount of processing time spent in
//  152 // function halSleep(). The first value is determined by measuring the number of
//  153 // sleep timer ticks from the beginning of the function to entering sleep mode.
//  154 // The second value is determined by measuring the number of sleep timer ticks
//  155 // from exit of sleep mode to the call to osal_adjust_timers().
//  156 #if defined( CC2541) || defined( CC2541S )
//  157 #define HAL_SLEEP_ADJ_TICKS                 25                    // default sleep adjustment, in 32kHz ticks
//  158 #else // CC2540
//  159 #define HAL_SLEEP_ADJ_TICKS                 35                    // default sleep adjustment, in 32kHz ticks
//  160 #endif // CC2541 || CC2541S
//  161 
//  162 // sleep and external interrupt port masks
